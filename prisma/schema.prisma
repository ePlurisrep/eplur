generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String     @id @default(cuid())
  email String     @unique
  nodes UserNode[]
}

model UserNode {
  id       String @id @default(cuid())
  userId   String
  label    String
  type     String
  metadata Json?
}

model UserRelation {
  id       String @id @default(cuid())
  sourceId String
  targetId String
  relation String
}
